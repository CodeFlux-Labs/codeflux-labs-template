{
    "name": "evolog-expo-template",
    "version": "1.0.0",
    "description": "A template for Expo projects using React Query, Zustand and MMKV",
    "main": "index.tsx",
    "scripts": {
        "start:dev": "APP=ENV=development npx expo start",
        "start:qas": "APP=ENV=test npx expo start",
        "start:prd": "APP=ENV=production npx expo start",
        "build:dev": "eas build --profile development --platform android",
        "android": "npx expo run:android",
        "ios": "npx expo run:ios",
        "web": "npx expo start --web",
        "format": "prettier --write \"src/**/*.{js,ts,tsx}\""
    },
    "dependencies": {
        "@expo/vector-icons": "^14.0.4",
        "@hookform/resolvers": "^3.9.1",
        "@react-native-async-storage/async-storage": "1.23.1",
