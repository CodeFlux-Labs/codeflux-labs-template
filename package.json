        "react-native-web": "~0.19.13",
        "styled-components": "^6.1.13",
        "use-between": "^1.3.5",
        "zod": "^3.24.1",
        "zustand": "^5.0.2"
    },
    "devDependencies": {
        "@babel/core": "^7.20.0",
        "@react-native-community/cli": "latest",
        "@types/react": "~18.3.12",
        "@types/react-native": "^0.73.0",
